<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhou.demo.persist.mapper.categoryMapper">
    <resultMap id="baseResultMap" type="com.zhou.demo.persist.po.CategoryTreeVo">
        <result column="id" jdbcType="INTEGER" property="id"></result>
        <result column="name" jdbcType="VARCHAR" property="name"></result>
        <result column="parent_id" jdbcType="INTEGER" property="parentId"></result>
        <result column="status" jdbcType="TINYINT" property="status"></result>
    </resultMap>

    <select id="selectlist" resultMap="baseResultMap">
        select *
        from t_category
    </select>

</mapper>